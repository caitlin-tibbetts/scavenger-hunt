{
  "name": "scavenger-hunt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npx sort-package-json && npx prettier --write . && react-scripts build",
    "eject": "react-scripts eject",
    "start": "npx sort-package-json && npx prettier --write . && react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "animate.css": "^4.1.1",
    "firebase": "^9.22.2",
    "formik": "^2.4.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-card-flip": "^1.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-router-dom": "^6.13.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "store2": "^2.14.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "prettier": "^2.8.8",
    "sort-package-json": "^2.4.1"
  },
  "overrides": {
    "react-qr-reader": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
